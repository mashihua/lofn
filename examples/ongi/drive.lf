using async in library "async";
using page in library "domport";
using move in library "bi.animation.simple";

var flow = async {
	var box = page "box";
	await: move box, {x: 200, y: 200}, {x: 300, y: 300}, 500;
	await: move box, {x: 300, y: 300}, {x: 500, y: 200}, 1000;
};

flow().start();
